      "Skill§Acrobatic": {
        "text": "Once per her interruption Teela can perform a free 2 hex move action. This action can be made in any direction and cannot be interrupted."
      },
      "Skill§Always Vigilant": {
        "text": "At the end of the activation, if there are no enemies within 8″, gain a <i class=\"game-icon game-icon-spears\" style=\"color: #e94\"></i>."
      },
      "Skill§Cold-Blooded": {
        "text": "Whiplash cannot perform a focus action. Each time an ally receives at least 1 <i class=\"game-icon game-icon-broken-heart\" style=\"color:#c24\"></i>, Whiplash gains a <i class=\"game-icon game-icon-convergence-target\" style=\"color: #a8c\"></i>."
      },
      "Skill§Dark Presence": {
        "text": "Test <i class=\"game-icon game-icon-spiky-explosion\"></i> against the <i class=\"game-icon game-icon-hood\"></i> of an enemy within 16″.  \n1 <i class=\"game-icon game-icon-round-star\"></i>: <i class=\"game-icon game-icon-bleeding-eye\" style=\"color: #393\"></i>."
      },
      "Skill§Evil Master of Odors": {
        "text": "At the end of activation, remove all <i class=\"game-icon game-icon-potion-ball\" style=\"color: #78c\"></i> and test <i class=\"game-icon game-icon-strong\"></i> against <i class=\"game-icon game-icon-hood\"></i> of all other characters within 1 (+ 1 per <i class=\"game-icon game-icon-potion-ball\" style=\"color: #78c\"></i> removed) hexes.  \n1 <i class=\"game-icon game-icon-round-star\"></i>: <i class=\"game-icon game-icon-bleeding-eye\" style=\"color: #393\"></i> or <i class=\"game-icon game-icon-knockout\" style=\"color:#393\"></i> or <i class=\"game-icon game-icon-foot-trip\" style=\"color:#393\"></i>."
      },
      "Skill§Eye of the Bee": {
        "text": "All tests with a <i class=\"game-icon game-icon-convergence-target\" style=\"color: #a8c\"></i>, gain +1 <i class=\"game-icon game-icon-perspective-dice-six-faces-five\"></i>."
      },
      "Skill§Face of the Monster": {
        "text": "Gain <i class=\"game-icon game-icon-lizardman\" style=\"color:#dd5\"></i>. <i class=\"game-icon game-icon-lizardman\" style=\"color:#dd5\"></i> provides Man-E-Faces with +2 <i class=\"game-icon game-icon-strong\"></i> and -1 <i class=\"game-icon game-icon-hood\"></i>. Test <i class=\"game-icon game-icon-strong\"></i> against the <i class=\"game-icon game-icon-heavy-helm\"></i> of an enemy engaged with Man-E-Faces.  \n1 <i class=\"game-icon game-icon-round-star\"></i>: 1 <i class=\"game-icon game-icon-broken-heart\" style=\"color:#c24\"></i>.",
        "stats": {
          "AP": {
            "value": 1
          },
          "Mana": {
            "value": 1
          }
        }
      },
      "Skill§Face of the Robot": {
        "text": "Gain <i class=\"game-icon game-icon-cyborg-face\" style=\"color:#dd5\"></i> and a <i class=\"game-icon game-icon-convergence-target\" style=\"color: #a8c\"></i>. <i class=\"game-icon game-icon-cyborg-face\" style=\"color:#dd5\"></i> provides Man-E-Faces with +2 <i class=\"game-icon game-icon-crystal-eye\"></i> and -1 <i class=\"game-icon game-icon-heavy-helm\"></i>.",
        "stats": {
          "AP": {
            "value": 1
          },
          "Mana": {
            "value": 1
          }
        }
      },
      "Skill§Fearsome Presence": {
        "text": "If any character ends their activation or interruption within 2 hexes from Scareglow, test <i class=\"game-icon game-icon-spiky-explosion\"></i> against <i class=\"game-icon game-icon-hood\"></i> of that character.  \n1 <i class=\"game-icon game-icon-round-star\"></i>: <i class=\"game-icon game-icon-tear-tracks\" style=\"color:#393\"></i>."
      },
      "Skill§Freezing Ray": {
        "text": "Test <i class=\"game-icon game-icon-spiky-explosion\"></i>. +1 against the <i class=\"game-icon game-icon-hood\"></i> of an enemy within 12″.  \n1: <i class=\"game-icon game-icon-round-star\"></i>: <i class=\"game-icon game-icon-foot-trip\" style=\"color:#393\"></i>"
      },
      "Skill§Gammavision": {
        "stats": {
          "Mana": {
            "value": 2
          }
        },
        "text": "Test <i class=\"game-icon game-icon-spiky-explosion\"></i> + 2 against the <i class=\"game-icon game-icon-hood\"></i> of an enemy within 12″.  \n1 <i class=\"game-icon game-icon-round-star\"></i>: <i class=\"game-icon game-icon-jawless-cyclop\" style=\"color:#393\"></i>, all attacks against a character with a <i class=\"game-icon game-icon-jawless-cyclop\" style=\"color:#393\"></i> gain a <i class=\"game-icon game-icon-convergence-target\" style=\"color: #a8c\"></i>."
      },
      "Skill§Grappling Hook": {
        "text": "If Trap Jaw has the ‘Hook Attachment’ equipped, test <i class=\"game-icon game-icon-crystal-eye\"></i> -1.  \nTrap Jaw can jump in a straight line up to 2 hexes per <i class=\"game-icon game-icon-round-star\"></i>.",
        "stats": {
          "Mana": {
            "value": 2
          }
        }
      },
      "Skill§Hero in the Sky": {
        "text": "Each time an ally gets a <i class=\"game-icon game-icon-convergence-target\" style=\"color: #a8c\"></i>, Buzz-Off can immediately jump 1 hex. This movement cannot be interrupted."
      },
      "Skill§Human Battering Ram": {
        "text": "If Ram Man ends his activation engaged, he gains a <i class=\"game-icon game-icon-convergence-target\" style=\"color: #a8c\"></i>."
      },
      "Skill§I Have the Power!": {
        "text": "During any test, He-Man may reroll 1 extra <i class=\"game-icon game-icon-perspective-dice-six-faces-five\"></i> for each <i class=\"game-icon game-icon-potion-ball\" style=\"color: #78c\"></i> spent on rerolls."
      },
      "Skill§Instant Puddle": {
        "text": "Test <i class=\"game-icon game-icon-spiky-explosion\"></i>.  \n1: <i class=\"game-icon game-icon-round-star\"></i>: Place a <i class=\"game-icon game-icon-triton-head\" style=\"color:#f60\"></i> within 8″. The area within 2 hexes from a <i class=\"game-icon game-icon-triton-head\" style=\"color:#f60\"></i> is treated as difficult terrain."
      },
      "Skill§Leader of Avion": {
        "stats": {
          "AP": {
            "value": 1
          },
          "Mana": {
            "value": 1
          }
        },
        "text": "Test <i class=\"game-icon game-icon-spiky-explosion\"></i>.   \n1 <i class=\"game-icon game-icon-round-star\"></i>: Choose any activated ally (line of sight not required). This ally gains a <i class=\"game-icon game-icon-convergence-target\" style=\"color: #a8c\"></i>. or an <i class=\"game-icon game-icon-spears\" style=\"color: #e94\"></i>. This ability cannot be used during an interruption."
      },
      "Skill§Lord of Destruction": {
        "stats": {
          "Mana": {
            "value": 1
